{
    "name": "templategenius/template-genius-laravel",
    "description": "This is my package template-genius-laravel",
    "keywords": [
        "TemplateGenius",
        "laravel",
        "template-genius-laravel"
    ],
    "homepage": "https://github.com/templategenius/template-genius-laravel",
    "license": "MIT",
    "authors": [
        {
            "name": "Isak Berglind",
            "email": "info@isakberglind.se",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "doctrine/dbal": "^3.6",
        "guzzlehttp/guzzle": "^7.7",
        "illuminate/contracts": "^10.0",
        "spatie/laravel-package-tools": "^1.14.0"
    },
    "require-dev": {
        "laravel/pint": "^1.0",
        "nunomaduro/collision": "^7.9",
        "nunomaduro/larastan": "^2.0.1",
        "orchestra/testbench": "^8.0",
        "pestphp/pest": "^2.0",
        "pestphp/pest-plugin-arch": "^2.0",
        "pestphp/pest-plugin-laravel": "^2.0",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan-deprecation-rules": "^1.0",
        "phpstan/phpstan-phpunit": "^1.0",
        "spatie/laravel-ray": "^1.26"
    },
    "autoload": {
        "psr-4": {
            "TemplateGenius\\TemplateGenius\\": "src/",
            "TemplateGenius\\TemplateGenius\\Database\\Factories\\": "database/factories/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": "@php ./vendor/bin/testbench package:discover --ansi",
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "TemplateGenius\\TemplateGenius\\TemplateGeniusServiceProvider"
            ],
            "aliases": {
                "TemplateGenius": "TemplateGenius\\TemplateGenius\\Facades\\TemplateGenius"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
